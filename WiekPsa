using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Podaj wiek psa Norman:");
        int wiekNormana = Convert.ToInt32(Console.ReadLine());

        int wiekLudzki = wiekNormana * 7;

        Console.WriteLine("Wiek Normana w ludzkich latach to:");
        Console.WriteLine(wiekLudzki);
    }
}
